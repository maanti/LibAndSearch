DROP TABLE sources;
CREATE TABLE sources
(
  id        SERIAL  NOT NULL
    CONSTRAINT sources_pkey
    PRIMARY KEY,
  name      VARCHAR,
  author    VARCHAR,
  publisher VARCHAR,
  year      INTEGER,
  type      VARCHAR,
  link      VARCHAR,
  path      VARCHAR,
  tags      VARCHAR
);
